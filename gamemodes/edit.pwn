publics: OffBan(playerid,tmp[])
{
	new year, month,day,mysql_query[144];
	getdate(year, month, day);
	new curdate = getdate(year,month,day),rows, fields,stringer[144];
	cache_get_data(rows, fields);
	if(!rows) return SendMes(playerid, COLOR_GREY, "[ MySql ] Аккаунт %s не найден в базе данных.",tmp);
	format(mysql_query, sizeof(mysql_query), "UPDATE `accounts` SET `pLocked2` = '1',`pBanData1` = '%d', `pBanTime1` = '%d' WHERE `Name` = '%s'",curdate+7,curdate, tmp);
	mysql_function_query(connects, mysql_query, false, "", "");
	SendMes(playerid, COLOR_GREY, "[ MySql ] Аккаунт %s успешно забанен на 7 дней.", tmp);
	format(stringer, sizeof(stringer), "OffBan[забанил: %s][забанен: %s][причина: %s]",PlayerInfo[playerid][pNames],tmp);
	ABroadCast(COLOR_LIGHTRED,stringer,1);
	return true;
}

