}

publics: UnBan(playerid,tmp[])
{
	new rows, fields,tmp1[2],stringer[144],mysql_query[128];
	cache_get_data(rows, fields);
	if(!rows) return SendMes(playerid, COLOR_GREY, "[ MySql ] Аккаунт %s не найден в базе данных.",tmp);
	cache_get_field_content(0, "pBanKol", tmp1);
	format(mysql_query, sizeof(mysql_query), "UPDATE `accounts` SET `pLocked2` = '0',`pBanData1` = '0', `pBanTime1` = '0', `pBanKol` = '%d' WHERE `Name` = '%s'",strval(tmp)-1,tmp);
	mysql_function_query(connects, mysql_query, false, "", "");
	SendMes(playerid, COLOR_GREY, "[ MySql ] Аккаунт %s успешно разбанен", tmp);
	format(stringer, sizeof(stringer), "[UNBAN] {FFFFFF}Администратор {FF6347}%s {FFFFFF}разбанил {FF6347}%s",PlayerInfo[playerid][pNames],tmp);
	ABroadCast(COLOR_LIGHTRED,stringer,1);
	return true;
}

