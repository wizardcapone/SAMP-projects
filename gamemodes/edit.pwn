stock GetClosestPlayer(p1)
{
	new Float:dis,Float:dis2,player;
	player = INVALID_PLAYER_ID;
	dis = 99999.99;
	foreach(Player,x)
	{
		if(IsPlayerConnected(x))
		{
			if(x != p1)
			{
				dis2 = GetDistanceBetweenPlayers(x,p1);
				if(dis2 < dis && dis2 != -1.00) dis = dis2,player = x;
			}
		}
	}
	return player;
}
stock PayDay()
{
	for(new i =0;i<sizeof(GZInfo);i++)
	{
		if(GZInfo[i][gFrakVlad] == 18) TotalGzR++;
		if(GZInfo[i][gFrakVlad] == 17) TotalGzA++;
		if(GZInfo[i][gFrakVlad] == 15) TotalGzG++;
		if(GZInfo[i][gFrakVlad] == 13) TotalGzV++;
		if(GZInfo[i][gFrakVlad] == 12) TotalGzB++;
	}
	FracBank[0][fGrove] += TotalGzG*300;
	FracBank[0][fBallas] += TotalGzB*300;
	FracBank[0][fVagos] += TotalGzV*300;
	FracBank[0][fAztek] += TotalGzA*300;
	FracBank[0][fRifa] += TotalGzR*300;
	ClearTotalGz();
	CaptureCommand[0] = 0;
	CaptureCommand[1] = 0;
	if(armmatbi < 0) armmatbi = 0;
	else if(armmatsf < 0) armmatsf = 0;
	else if(kazna < 0) kazna = 0;
	else if(lspdmatbi < 0) lspdmatbi = 1000;
	else if(sfpdmats < 0) sfpdmats = 1000;
	else if(lvpdmats < 0) lvpdmats = 1000;
	else if(fbimats < 0) fbimats = 1000;
	else if(yakuzamats < 0) yakuzamats = 1000;
	else if(rmmats < 0) rmmats = 1000;
	else if(lcnmats < 0) lcnmats = 1000;
	else if(rifamatbi < 0) rifamatbi = 1000;
	else if(ballasmatbi < 0) ballasmatbi = 1000;
	else if(groovmatbi < 0) groovmatbi = 1000;
	else if(vagosmatbi < 0) vagosmatbi = 1000;
	else if(aztekmatbi < 0) aztekmatbi = 1000;
	/////////
	foreach(Player,i)
	{
	    if(PlayerInfo[i][pTimeServer] < 1200) {
			SendMes(i, COLOR_GREY, "Вы отыграли менее 20 минут и не получили PayDay!");
			continue;
		}
		else if(GetPVarInt(i, "AFK_Time") > 1) {
			SendMes(i, COLOR_GREY, "Вы простояли в режиме АФК во время PayDay и не получили его!");
			continue;
		}
		if(PlayerInfo[i][pWanted] >= 1)
		{
			WantedLevel[i] -= 1;
			PlayerInfo[i][pWanted] -=1;
			SetPlayerWantedLevel(i, PlayerInfo[i][pWanted]);
		}
		new exp = PlayerInfo[i][pExp];
		new nxtlevel = PlayerInfo[i][pLevel]+1;
		new expamount = nxtlevel*levelexp;
		if(exp == expamount)
		{
			PlayerInfo[i][pLevel] += 1;
			PlayerInfo[i][pExp] = 0;
			SendClientMessage(i,COLOR_GREEN,"Поздравляем! Ваш игровой lvl повысился.");
		}
		new moneys = 1+random(8000);
		new countgangs = 0;
		new countmongang = 0;
		for(new l =0;l<130;l++)
		{
			if(GZInfo[GZInfo[l][gID]][gFrakVlad] == PlayerInfo[i][pMember])
			{
				countgangs++;
			}
		}
		countmongang = countgangs * 200;
		switch(PlayerInfo[i][pMember])
        {
