stock OnPlayerLogin(playerid, password[])
{
    new mysql_query[144],sendername[24];
    GetPlayerName(playerid,sendername,24);
	mysql_format(connects,mysql_query, sizeof(mysql_query),"SELECT * FROM `accounts` WHERE `Name` = '%s' AND `pKey` = '%e'", sendername, password);
	mysql_function_query(connects, mysql_query, true, "LoginCallback","ds", playerid, password);
	return true;
}
publics: OffWarn(playerid,tmp[])
{
	new rows, fields,stringer[144],mysql_query[128];
	cache_get_data(rows, fields);
	if(!rows) return SendMes(playerid, COLOR_GREY, "[ MySql ] Аккаунт %s не найден в базе данных.",tmp);
	format(mysql_query, sizeof(mysql_query), "UPDATE `accounts` SET `pWarns` = '1', `pLeader` = '0', `pMember` = '0', `pRank` = '0' WHERE `Name` = '%s'", tmp);
	mysql_function_query(connects, mysql_query, false, "", "");
	SendMes(playerid, COLOR_GREY, "[ MySql ] Аккаунт %s успешно был предупреждён(warn).", tmp);
	format(stringer, sizeof(stringer), "OffWarn [заварнил: %s] [заварнен: %s]",PlayerInfo[playerid][pNames],tmp);
	ABroadCast(COLOR_LIGHTRED,stringer,1);
	return true;
}

publics: Delete(playerid,tmp[])
{
	new rows, fields,stringer[84],mysql_query[128];
	cache_get_data(rows, fields);
