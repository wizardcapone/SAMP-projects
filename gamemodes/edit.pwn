        if(opentext[playerid] == 1)
        {
            for(new j=0; j<30; j++)
		    {
		        PlayerTextDrawDestroy(playerid, NewTextPTD[playerid][j]);
		        CancelSelectTextDraw(playerid);
		    }
		    opentext[playerid] = 0;
        }
        else if(opentext[playerid] == 2)
        {
            for(new j=0; j<75; j++)
		    {
		        PlayerTextDrawDestroy(playerid, TDEditor_PTD[playerid][j]);
		        CancelSelectTextDraw(playerid);
		    }
		    opentext[playerid] = 0;
        }
    }
	return 1;
}
