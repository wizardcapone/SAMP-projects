						SendClientMessage(playerid, COLOR_GRAD1, "У вас нет столько денег!");
						ShowMenuForPlayer(skinshopmagaz[0],playerid);
						return true;
					}
					if(CharPrice[playerid] == 2000000) PlayerInfo[playerid][pCash]-=2000000;
					else if(CharPrice[playerid] == 10000) PlayerInfo[playerid][pCash]-=10000;
					else if(CharPrice[playerid] == 50000) PlayerInfo[playerid][pCash]-=50000;
					else if(CharPrice[playerid] == 4000000) PlayerInfo[playerid][pCash]-=4000000;
					else if(CharPrice[playerid] == 1000000) PlayerInfo[playerid][pCash]-=1000000;
					if(zips[playerid] == true)//зип
					{
						SetPlayerInterior(playerid,18);
						PlayerInfo[playerid][pInt] = 18;
						CharPrice[playerid] = 0;
						PlayerInfo[playerid][pCash]-=CharPrice[playerid];
						SetPPos(playerid ,181.1844,-88.0599,1002);
						TogglePlayerControllable(playerid,1);
						SetPlayerVirtualWorld(playerid,0);
						PlayerInfo[playerid][pChar] = ChosenSkin[playerid];
						SetCameraBehindPlayer(playerid);
						SelectCharPlace[playerid] = 0;
						SelectCharID[playerid] = 0;
						SelectChar[playerid] = 0;
						SetPlayerSkin(playerid, PlayerInfo[playerid][pChar]);
						return true;
					}
					CharPrice[playerid] = 0;
					PlayerInfo[playerid][pCash]-=CharPrice[playerid];
					SetPPos(playerid ,209.4333,-7.9053,1001.2109);
					TogglePlayerControllable(playerid,1);
					SetPlayerVirtualWorld(playerid,0);
					PlayerInfo[playerid][pChar] = ChosenSkin[playerid];
					SetCameraBehindPlayer(playerid);
					SelectCharPlace[playerid] = 0;
					SelectCharID[playerid] = 0;
					SelectChar[playerid] = 0;
					SetPlayerSkin(playerid, PlayerInfo[playerid][pChar]);
				}
			case 2://exit
				{
					if(zips[playerid] == true)//зип
					{
						SetPlayerInterior(playerid,18);
						PlayerInfo[playerid][pInt] = 18;
						SetPlayerVirtualWorld(playerid,0);
						CharPrice[playerid] = 0;
						TogglePlayerControllable(playerid,1);
						SetCameraBehindPlayer(playerid);
						PlayerInfo[playerid][pChar] = PlayerInfo[playerid][pChar];
						SetPPos(playerid ,181.1844,-88.0599,1002);
						SetPlayerVirtualWorld(playerid,0);
						SetPlayerSkin(playerid, OldSkin[playerid]);
						PlayerInfo[playerid][pChar] = OldSkin[playerid];
						SelectCharPlace[playerid] = 0;
						SelectCharID[playerid] = 0;
						SelectChar[playerid] = 0;
						return true;
					}
					SetPlayerVirtualWorld(playerid,0);
					CharPrice[playerid] = 0;
					TogglePlayerControllable(playerid,1);
					SetCameraBehindPlayer(playerid);
					PlayerInfo[playerid][pChar] = PlayerInfo[playerid][pChar];
					SetPPos(playerid ,209.4333,-7.9053,1001.2109);
					SetPlayerVirtualWorld(playerid,0);
					SetPlayerSkin(playerid, OldSkin[playerid]);
					PlayerInfo[playerid][pChar] = OldSkin[playerid];
					SelectCharPlace[playerid] = 0;
					SelectCharID[playerid] = 0;
					SelectChar[playerid] = 0;
				}
			}
		}
		return true;
	}
	return true;
}
